<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mapper.test">
	<insert id="insert">
		insert into test (num,tname)
		values
		(#{num},#{tname})
	</insert>
	<select id="list" resultType="kr.co.domain.TestDTO">
		select * from test
	</select>
	<select id="read" resultType="kr.co.domain.TestDTO">
		select * from test where num =
		#{num}
	</select>
	<update id="update">
		update test set tname = #{tname} where
		num = #{num}
	</update>
	<delete id="delete">
		delete test where num = #{num}
	</delete>
</mapper>