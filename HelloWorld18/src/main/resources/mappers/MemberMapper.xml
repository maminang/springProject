<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mapper.member">

	<insert id="insert">
	insert into tbl_member (id,pw,name,email,address,phone,birth) values 
	(#{id},#{pw},#{name},#{email},#{address},#{phone},#{birth})
	</insert>

	
</mapper>